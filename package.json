{
  "name": "golangci-lint-npm",
  "version": "0.1.0",
  "description": "An npm wrapper for golangci-lint.",
  "keywords": [
    "golangci-lint",
    "lint",
    "golang",
    "npm",
    "wrapper",
    "cross-platform"
  ],
  "homepage": "https://github.com/ishizakiyu/golangci-lint-npm",
  "bugs": {
    "url": "https://github.com/ishizakiyu/golangci-lint-npm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishizakiyu/golangci-lint-npm.git"
  },
  "license": "MIT",
  "author": "Yu Ishizaki <yizk.dev@gmail.com>",
  "type": "module",
  "bin": {
    "golangci-lint": "bin/golangci-lint.js"
  },
  "files": [
    "/bin/golangci-lint.js"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "fallbackGolangciVersion": "2.1.6"
}
